class Solution {
    public int[] runningSum(int[] nums) {
    int temp=0;

        int n=nums.length;
        int [] arr=new int[n];
        for (int i=0;i<n;i++){
            temp=temp+nums[i];
            arr[i]=temp;
            
        }
        
        return arr;
        
        
        
    }
}
