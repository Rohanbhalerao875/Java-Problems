

//User function Template for Java


class Solution {
    ArrayList<Integer> valueEqualToIndex(int arr[], int n) {
        // code here
        int i=0;
        ArrayList<Integer> ans = new ArrayList<Integer>();
	    for (i=0;i<n;i++){
	        if(arr[i]==i+1){

	            ans.add(i+1);
	        }
	    }
	    return ans ;
    }
}
